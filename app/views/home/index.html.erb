<%= content_for(:head) do %>
  <% if should_load_css? %>
    <link rel="stylesheet" href="<%= asset_url('home.css') %>">
  <% end %>
<% end %>

<div id="root"></div>

<%= content_for(:scripts) do %>
  <script type="application/json" id="bookmarks-data">
    <%= raw(bookmarks.to_json) %>
  </script>
  <script crossorigin src="<%= asset_url('home.js') %>"></script>
<% end %>
