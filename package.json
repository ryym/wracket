{
  "name": "wracket",
  "private": true,
  "author": "ryym <ryym.64@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "build:prod": "NODE_ENV=production webpack",
    "build:analyze": "NODE_ENV=production BUNDLE_ANALYZE=1 webpack",
    "lint": "yarn lint:js && yarn lint:ts && yarn lint:css",
    "lint:js": "eslint .",
    "lint:ts": "tslint -p . -t verbose 'frontend/**/*.ts?(x)'",
    "lint:css": "stylelint 'frontend/**/*css'",
    "lint:css:fix": "stylelint --fix 'frontend/**/*css'",
    "test": "jest",
    "test:w": "yarn test --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npx prettier --write",
      "npx eslint"
    ],
    "*.{ts,tsx}": [
      "npx prettier --write",
      "npx tslint -p tsconfig.precommit.json"
    ],
    "*.{css,scss}": [
      "npx stylelint",
      "npx prettier --write"
    ],
    "*.{rb,rake}": [
      "bundle exec rubocop -a --force-exclusion"
    ]
  },
  "//": [
    "NOTE: @types/react-dom depends on @types/node",
    "NOTE: Using @types/react v16.4 and @types/react-dom v16.0 causes an error",
    "      https://github.com/DefinitelyTyped/DefinitelyTyped/issues/24711"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.4",
    "@types/enzyme": "^3.1.10",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.10",
    "@types/node": "^8.10.10",
    "@types/react": "^16.4.0",
    "@types/react-dom": "^16.4.0",
    "@types/react-redux": "^7.0.9",
    "@types/react-router": "^4.4.3",
    "@types/react-transition-group": "^2.0.14",
    "@types/redux-logger": "^3.0.5",
    "autoprefixer": "^9.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.10",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^2.9.0",
    "husky": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.11.1",
    "react-transition-group": "^2.5.0",
    "sass-loader": "^7.0.1",
    "sinon": "^5.0.3",
    "style-loader": "^0.20.2",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.9.1",
    "typescript": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.12.0",
