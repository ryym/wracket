{
  "name": "wracket",
  "private": true,
  "author": "ryym <ryym.64@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:js": "eslint .",
    "lint:ts": "tslint -p . -t verbose 'frontend/**/*.ts?(x)'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npx prettier --write",
      "npx eslint",
      "git add"
    ],
    "*.{ts,tsx}": [
      "npx prettier --write",
      "npx tslint",
      "git add"
    ],
    "*.{css,scss}": [
      "npx prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.5.0",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.10",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "style-loader": "^0.20.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "3.5.4",
    "webpack-dev-server": "^2.7.0",
    "webpack-manifest-plugin": "^1.3.1"
  },
  "dependencies": {
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/redux-logger": "^3.0.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "4.0.0-beta.2",
    "redux-logger": "^3.0.6"
  },
  "// XXX: NOTE": [
    "Currently we are using redux-dutiful-redux by 'yarn link'.",
    "(https://github.com/ryym/redux-dutiful-thunk)"
  ]
}
